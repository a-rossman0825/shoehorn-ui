{
  "name": "shoehorn-ui",
  "version": "0.1.3",
  "description": "Design-agnostic Vue 3 component library",
  "type": "module",
  "license": "MIT",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/style.css",
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./dist/style.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc -p tsconfig.json",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/ui": "^4.0.17",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^27.4.0",
    "sass": "^1.97.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.17",
    "vue": "^3.5.26"
  }
}
